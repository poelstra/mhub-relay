{
	"servers": {
		"local": "ws://localhost:13900",
		"remote": "ws://localhost:13901"
	},
	"bindings": {
		"default": {
			"filters": [
				{ "from": "local/proxy", "pattern": "twitter:*" },
				{ "from": "local/proxy", "pattern": "something:*" }
			],
			"target": "remote/proxy"
		},
		"overlay": {
			"filters": [
				{ "from": "local/overlay" }
			],
			"target": "remote/overlay"
		}
	}
}
